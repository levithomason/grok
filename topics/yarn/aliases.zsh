alias yi='yarn install'
alias ya='yarn add $1'
alias yad='yarn add $1 --save-dev'
alias yr='yarn remove'
alias ys='yarn start'
alias ysw='yarn start:watch'
alias yt='yarn test'
alias ytw='yarn test:watch'
alias yl='yarn lint'
alias ylf='yarn lint:fix'
alias ylw='yarn lint:watch'
alias yb='yarn build'
alias ybw='yarn build:watch'
